<topic name="Kissen">

<category>
    <pattern>Nein</pattern>
    <template>
        <link>
            <text>Hier gehts zu deinen Produkten!</text>
            <url>https://www.opti-wohnwelt.de/deko-textil/heimtextilien/kissen.html</url>
